<Page x:Class="VkWPF.Pages.PageFriends"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:VkWPF.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="PageFriends">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="6*"/>
        </Grid.ColumnDefinitions>
        <Frame Grid.Column="0" x:Name="frameFriends" />
        <ComboBox x:Name="comboYearsFilter" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="160" Margin="10,0,0,0" DropDownClosed="comboYearsFilter_DropDownClosed"/>
    </Grid>
</Page>
